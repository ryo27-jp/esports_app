<%= form_with model: team, local: true do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, autofocus: true, class:"form-control" %>
  </div>

  <div>
    <%= f.label :about %>
    <%= f.text_area :about, class:"form-control" %>
  </div>

  <div class="field">
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit class:"btn btn-primary my-2" %>
  </div>
<% end %>